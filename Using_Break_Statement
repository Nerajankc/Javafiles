package com.mycompany.testingmain;

/**
 *
 * @author niraj
 */
public class Using_Break {
    public static void main(String[] args)
    {
        int x; 
        for(x=1 ; x<= 10 ; x++)
        { 
            if(x==6)
                break;
            System.out.println(x);
             
        }
    }
    
}
